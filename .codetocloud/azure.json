{
  "projectType": "azd",
  "iacTypes": [
    "bicep"
  ],
  "resources": [
    {
      "type": "azurecontainerapp",
      "instances": [
        {
          "name": "src"
        }
      ]
    },
    {
      "type": "azurefunctions",
      "instances": [
        {
          "name": "functions0"
        }
      ]
    },
    {
      "type": "azurecosmosdb",
      "instances": [
        {
          "name": "db0"
        }
      ]
    }
  ],
  "bindings": [
    {
      "source": "${azurecontainerapp.src}",
      "target": "${azurefunctions.functions0}",
      "store": "",
      "customKeys": {}
    },
    {
      "source": "${azurecontainerapp.src}",
      "target": "${azurecosmosdb.db0}",
      "connection": "system-identity",
      "store": "",
      "customKeys": {
        "AZURE_COSMOS_RESOURCEENDPOINT": "AZURE_COSMOS_ENDPOINT"
      }
    }
  ],
  "services": [
    {
      "host": "${azurecontainerapp.src}",
      "language": "dotnet",
      "project": "src/",
      "port": "80"
    }
  ],
  "settings": []
}